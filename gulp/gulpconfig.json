{

	"css": {
		"watch": "src/less/**/*.less",
		"src": "src/less/main.less",
		"dist": "dist/css",
		"id": "main"
	},

	"js": {
		"vendor": [
			"src/js/vendor/**/*.js"
		],
		"src": [
			"src/js/main.js"
		],
		"dist": "dist/js",
		"id": "main"
	},

	"templates": {
		"src": "src/js/templates/**/*.hbs",
		"dist": "src/js",
		"id": "templates"
	},

	"svg": {
		"src": "src/img/**/*.svg",
		"dist": "dist/img",
		"id": "main"
	},

	"rev": {
		"dist": "dist"
	},

	"browser": {
		"host": "localhost"
	}

}
